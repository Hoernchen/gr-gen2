<?xml version="1.0"?>
<block>
  <name>gen2 reader decoder</name>
  <key>rfid_reader_decoder</key>
  <category>RFID</category>
  <import>from gnuradio import gen2</import>
  <make>gen2.reader_decoder($uspers, $tari)</make>
  <param>
    <name>us per sample</name>
    <key>uspers</key>
    <type>float</type>
  </param>
  <param>
    <name>tari</name>
    <key>tari</key>
    <type>float</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
  </source>
</block>
